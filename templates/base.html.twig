<!DOCTYPE html>
<html lang="fr">
    <head>
        <meta charset="UTF-8">
            <title>{% block title %}Scion Interface{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml;base64,"/> <!-- shortened for brevity -->
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_link_tags('app') }}
            {{ encore_entry_script_tags('app') }}
            {% block importmap %}{{ importmap('app') }}{% endblock %}
        {% endblock %}
    </head>
    <body class="min-h-screen flex flex-col">
        {% include "partials/header.html.twig" %}
        <div class="flex-1 flex flex-col items-center p-4 background">
            {% block body %}{% endblock %}
        </div>
        {% include "partials/footer.html.twig" %}
    </body>
    <script>
        const checkEmailUrl = "{{ path('app_check_email') }}"
    </script>
    <script src="{{ asset('build/index.js') }}"></script>
    <script src="{{ asset('build/test.js') }}"></script>
</html>
